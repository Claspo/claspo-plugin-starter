{
  "name": "claspo-plugin-starter-poc",
  "scripts": {
    "install:all": "npm i && cd quickstart && cd backend && npm i && cd ../frontend && npm i",
    "components:build:prod": "npx webpack --config webpack.config.components.js",
    "components:build:dev": "cross-env dev=1 npx webpack --config webpack.config.components.js --watch",
    "components:dev": "cross-env dev=1 npx webpack serve --config webpack.config.components.js --watch",
    "quickstart:backend": "cd quickstart/backend && npm run dev",
    "quickstart:frontend": "cd quickstart/frontend && npm run dev",
    "dev": "concurrently --kill-others \"npm run components:dev\" \"npm run quickstart:backend\" \"npm run quickstart:frontend\" --names \"components,backend,frontend\" --prefix-colors \"cyan,magenta,yellow\""
  },
  "engines": {
    "node": ">=22.20.0"
  },
  "dependencies": {
    "@claspo/common": "6.0.6",
    "@claspo/document-connector": "15.0.5",
    "@claspo/renderer": "17.0.3",
    "@claspo/components": "1.1.18",
    "intl-tel-input": "^25.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@types/jest": "^27.5.1",
    "babel-loader": "^9.1.3",
    "compression": "^1.7.4",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^9.15.0",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^28.0.3",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.9.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  }
}
